{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "all": "yarn clean && yarn compile && yarn start",
    "compile": "tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage && rimraf tsconfig.tsbuildinfo",
    "codegen": "openapi-typescript http://localhost:3000/schema-json -o src/types.ts --export-type --immutable-types --support-array-length",
    "lint": "yarn eslint && yarn prettier",
    "eslint": "eslint --fix './**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "start": "node --max-http-header-size=24820 ./build/src/index.js",
    "test": "jest --coverage --passWithNoTests --maxWorkers=7",
    "ts-node": "ts-node",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/pg": "8.6.6",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/unzip-stream": "0.3.1",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "deepmerge": "4.3.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "express": "4.18.2",
    "express-openapi-validator": "5.0.3",
    "helmet": "6.0.1",
    "jest": "29.5.0",
    "openapi-typescript": "6.2.1",
    "prettier": "2.8.7",
    "rimraf": "4.4.1",
    "supertest": "6.3.3",
    "swagger-ui-express": "4.6.2",
    "ts-jest": "29.1.0",
    "typescript": "5.0.3"
  }
}
